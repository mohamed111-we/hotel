<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Room records -->
        <record id="ir_sequence_room" model="ir.sequence">
            <field name="name">Room</field>
            <field name="code">room.room</field>
            <field name="prefix">ROOM-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="ir_sequence_room_reservation" model="ir.sequence">
            <field name="name">Room Reservation</field>
            <field name="code">room.reservation</field>
            <field name="prefix">Reservation-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
